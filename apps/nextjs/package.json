		"@types/jest": "29.5.5",
		"@types/lodash": "4.14.202",
		"@types/node": "18.0.0",
		"@types/react": "18.2.34",
		"@types/react-dom": "18.2.7",
		"babel-loader": "9.1.3",
		"babel-plugin-macros": "3.1.0",
		"dotenv-cli": "7.3.0",
