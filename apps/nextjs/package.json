{
	"name": "@acme/nextjs",
	"version": "0.1.0",
	"private": true,
	"babelMacros": {
		"twin": {
			"preset": "emotion",
			"config": "tailwind.config.ts"
		}
	},
	"scripts": {
		"build": "pnpm with-env next build",
		"clean": "git clean -xdf .next .turbo node_modules",
		"dev": "pnpm with-env next dev -p 3000",
		"dev:https": "pnpm with-env next dev -p 3000 & local-ssl-proxy --key ./certificates/localhost-key.pem --cert ./certificates/localhost.pem --source 443 --target 3000",
		"lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
		"format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
		"start": "pnpm with-env next start",
		"typecheck": "tsc --noEmit",
		"test": "jest --watch",
		"test:ci": "jest --ci",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@acme/api": "^0.1.0",
		"@acme/auth": "^0.1.0",
		"@acme/db": "^0.1.0",
		"@acme/tailwind-config": "0.1.0",
		"@acme/ui": "^0.1.0",
		"@acme/validations": "0.1.0",
		"@emotion/cache": "^11.11.0",
		"@emotion/css": "^11.11.2",
		"@emotion/react": "^11.11.1",
		"@emotion/server": "11.11.0",
		"@emotion/styled": "^11.11.0",
		"@fontsource/work-sans": "^4.5.14",
		"@hookform/resolvers": "^3.3.1",
		"@t3-oss/env-nextjs": "^0.7.0",
		"@tanstack/react-query": "^4.35.7",
		"@tanstack/react-query-devtools": "^4.35.7",
		"@tanstack/react-query-next-experimental": "5.0.0-alpha.80",
		"@trpc/client": "^10.41.0",
		"@trpc/next": "^10.41.0",
		"@trpc/next-layout": "0.1.0",
		"@trpc/react-query": "^10.41.0",
		"@trpc/server": "^10.41.0",
		"accept-language": "3.0.18",
		"cookies-next": "^2.1.1",
		"i18next": "23.6.0",
		"i18next-browser-languagedetector": "7.1.0",
		"i18next-http-backend": "^2.3.1",
		"i18next-resources-to-backend": "1.1.4",
		"immer": "^9.0.19",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"next": "^13.5.6",
		"next-international": "^1.1.3",
		"nextjs-toploader": "^1.6.4",
		"react": "18.2.0",
		"react-cookie": "6.1.1",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.47.0",
		"react-i18next": "13.3.1",
