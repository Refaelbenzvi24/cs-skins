		"babel-loader": "^9.1.3",
		"babel-plugin-macros": "^3.1.0",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.50.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"js-yaml-loader": "^1.2.2",
		"prettier": "^3.0.3",
		"tailwindcss": "3.3.3",
		"tailwindcss-rtl": "^0.9.0",
		"ts-jest": "^29.1.1",
