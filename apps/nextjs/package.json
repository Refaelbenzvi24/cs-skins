{
	"name": "@acme/nextjs",
	"version": "0.1.0",
	"private": true,
	"babelMacros": {
		"twin": {
			"preset": "emotion",
			"config": "tailwind.config.ts"
		}
	},
	"scripts": {
		"build": "NODE_OPTIONS=--enable-source-maps pnpm with-env next build",
		"clean": "git clean -xdf .next .turbo node_modules",
		"dev": "pnpm with-env next dev -p 3000",
		"dev:https": "pnpm with-env next dev -p 3000 & local-ssl-proxy --key ./certificates/localhost-key.pem --cert ./certificates/localhost.pem --source 443 --target 3000",
		"lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
		"format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
		"start": "pnpm with-env next start",
		"typecheck": "tsc --noEmit",
		"test": "jest --watch",
		"test:ci": "jest --ci",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@acme/api": "0.1.0",
		"@acme/auth": "0.1.0",
		"@acme/db": "0.1.0",
		"@acme/logger": "0.1.0",
		"@acme/tailwind-config": "0.1.0",
		"@acme/ui": "0.1.0",
		"@acme/validations": "0.1.0",
		"@elastic/apm-rum": "5.16.0",
		"@elastic/apm-rum-react": "2.0.2",
		"@emotion/cache": "11.11.0",
		"@emotion/css": "11.11.2",
		"@emotion/react": "11.11.1",
		"@emotion/server": "11.11.0",
		"@emotion/styled": "11.11.0",
		"@fontsource/work-sans": "4.5.14",
		"@hookform/resolvers": "3.3.1",
		"@nivo/core": "0.83.0",
		"@nivo/line": "0.83.0",
		"@t3-oss/env-nextjs": "0.7.0",
		"@tanstack/react-query": "5.8.7",
		"@tanstack/react-query-devtools": "5.8.7",
		"@tanstack/react-query-next-experimental": "5.8.7",
		"@trpc/client": "11.0.0-next-beta.264",
		"@trpc/react-query": "11.0.0-next-beta.264",
		"@trpc/server": "11.0.0-next-beta.264",
		"@vercel/speed-insights": "1.0.5",
