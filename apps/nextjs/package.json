		"d3-color": "3.1.0",
		"i18next": "23.6.0",
		"i18next-browser-languagedetector": "7.1.0",
		"i18next-http-backend": "2.3.1",
		"i18next-resources-to-backend": "1.1.4",
		"immer": "9.0.19",
		"lodash": "4.17.21",
		"moment": "2.29.4",
		"next": "13.5.6",
		"nextjs-toploader": "1.6.6",
		"react": "18.2.0",
		"react-cookie": "6.1.1",
