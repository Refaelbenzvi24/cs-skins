{
	"name" :            "@acme/db-playground",
	"private" :         true,
	"version" :         "0.1.0",
	"main" :            "index.ts",
	"types" :           "index.ts",
	"scripts" :         {
		"clean" :         "rm -rf .turbo node_modules",
		"lint" :          "eslint .",
		"format" :        "prettier --check \"**/*.{mjs,ts,md,json}\"",
		"db:playground" : "ts-node-dev -r dotenv/config index.ts",
		"typecheck" :     "tsc --noEmit"
	},
	"dependencies" :    {},
	"devDependencies" : {
		"@acme/db" :              "0.1.0",
		"@acme/eslint-config" :   "0.2.0",
		"@acme/prettier-config" : "^0.1.0",
		"@acme/tsconfig" :        "^0.1.0",
		"@types/node" :           "^18.0.0",
		"eslint" :                "^8.50.0",
		"nodemon" :               "^2.0.20",
		"ts-node-dev" :           "^2.0.0",
		"typescript" :            "^5.2.2"
	},
	"eslintConfig" :    {
		"root" :    true,
		"extends" : [
			"@acme/eslint-config/base"
		]
	},
	"prettier" :        "@acme/prettier-config"
}
